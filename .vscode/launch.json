    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "music.music",
                "FLASK_ENV": "development"
            },
            "envFile": "${workspaceFolder}/.env",
            "args": [
                "run",
                "--no-debugger"
            ],
            "jinja": true,
            "justMyCode": false
        },
        {
            "command": "npm run devbuild",
            "name": "Js: Build Dev",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "command": "npm run build",
            "name": "Js: Build Release",
            "request": "launch",
            "type": "node-terminal"
        }
    ]
}